<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.packathon">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PauseScreenActivity"></activity>
        <activity android:name=".GameOverActivity" />
        <activity android:name=".StartOfRoundActivity" />
        <activity android:name=".EndOfRoundActivity" />
        <activity android:name=".TurnActivity">
            <intent-filter>
                <action android:name="android.intent.action.TURN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RoundActivity" />
        <activity android:name=".ScoreboardActivity" />
        <activity android:name=".PlayerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>